@model IEnumerable<Web_Institute_Of_Fine_Arts_Database_Models.COURS_MODEL>

@{
    ViewBag.Title = "Course_List";
    Layout = "~/Views/Shared/Admin_Master_Layout/Admin_Layout.cshtml";
}

<h4>Course_List</h4>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CR_ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CR_NAME)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CR_ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CR_NAME)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CR_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.CR_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CR_ID })
        </td>
    </tr>
}

</table>
