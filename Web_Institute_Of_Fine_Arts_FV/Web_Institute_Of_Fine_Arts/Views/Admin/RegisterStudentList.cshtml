                @Html.DisplayFor(modelItem => item.STDL_PASSWORD)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.STD_ADDRESS)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.STD_EMAIL)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.STD_PHONE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.STD_DATE_OF_BIRTH)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
    }

</table>
