@{
    ViewBag.Title = "RegisterStudentList";
    Layout = "~/Views/Shared/Admin_Master_Layout/Admin_Layout.cshtml";
}

<h2>RegisterStudentList</h2>
@Html.Raw(TempData["datamessage"])
@Html.Raw(TempData["errormessage"])

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.STDL_ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.REG_ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.REG_CR_ID)
