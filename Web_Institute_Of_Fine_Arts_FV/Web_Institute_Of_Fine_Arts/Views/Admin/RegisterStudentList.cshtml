        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.STDL_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.REG_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.REG_CR_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.REG_DATE)
            </td>
